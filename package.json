{
      "name": "gestao-escolar-backend",
      "version": "1.0.0",
      "main": "index.js",
      "repository": "https://github.com/Dline-Code/gestao-escolar-backend.git",
      "author": "Dline-Code <dlinecode@gmail.com>",
      "license": "MIT",
      "scripts": {
            "dev": " nodemon --exec ts-node src/server.ts",
            "start": "ts-node src/server.ts",
            "typeorm": "npx ts-node ./node_modules/typeorm/cli.js",
            "migration:create": "ts-node ./node_modules/typeorm/cli.js migration:create -n",
            "migration:drop-create": "ts-node ./node_modules/typeorm/cli.js migration:drop && ts-node",
            "migration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate ",
            "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run ",
            "migration:show": "ts-node ./node_modules/typeorm/cli.js migration:show",
            "migration:rollback": "ts-node ./node_modules/typeorm/cli.js migration:revert"
      },
      "devDependencies": {
            "@types/express": "^4.17.13",
            "@types/node": "^8.0.29",
            "@types/bcrypt": "^5.0.0",
            "@types/jsonwebtoken": "^8.5.4",
            "typescript": "^4.3.5",
            "nodemon": "^2.0.8",
            "sucrase": "^3.19.0",
            "ts-node": "^10.1.0"
      },
      "dependencies": {
            "yup": "^0.32.9",
            "@types/typescript": "^2.0.0",
            "body-parser": "^1.18.1",
            "bcrypt": "^5.0.1",
            "dotenv": "^10.0.0",
            "express": "^4.17.1",
            "pg": "^8.6.0",
            "reflect-metadata": "^0.1.13",
            "@types/bcrypt": "^5.0.0",
            "jsonwebtoken": "^8.5.1",
            "typeorm": "^0.2.34"
      },
      "browserslist": {
            "production": [
                  ">0.2%",
                  "not dead",
                  "not op_mini all"
            ],
            "development": [
                  "last 1 chrome version",
                  "last 1 firefox version",
                  "last 1 safari version"
            ]
      }
}
